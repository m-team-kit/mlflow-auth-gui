# ELSE AAI Provider: AI4EOSC
OAUTH_INTROSPECTION_ENDPOINT=https://login.cloud.ai4eosc.eu/realms/ai4eosc/protocol/openid-connect/token/introspect
OAUTH_USERINFO_ENDPOINT=https://login.cloud.ai4eosc.eu/realms/ai4eosc/protocol/openid-connect/userinfo
REQUIRED_REALM_ROLES="vo.ai4eosc.eu,platform-access:vo.ai4eosc.eu"

NEXT_PUBLIC_OAUTH_AUTHORITY=https://login.cloud.ai4eosc.eu/realms/ai4eosc/
NEXT_PUBLIC_OIDC_CLIENT_ID=mlflow-dev-ai4eosc
NEXT_PUBLIC_OIDC_REDIRECT_URL=http://localhost:3000/signup


NEXT_PUBLIC_PRIVACY_POLICY_URL=https://ai4eosc.eu/platform/privacy-policy/
NEXT_PUBLIC_TERMS_OF_USE_URL=https://ai4eosc.eu/platform/acceptable-use-policy/